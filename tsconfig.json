{
  "compilerOptions": {
    "module": "esnext",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "Node",
    "declaration": true,
    "sourceMap": true,
    "outDir": ".",
    "declarationMap": false,
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ],
}